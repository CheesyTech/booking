{
  "name": "cheeasytech/booking",
  "description": "Implementation of multi-booking for Laravel",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Nikita Koida"
    }
  ],
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^10.0 || ^11.0 || ^12.0",
    "spatie/laravel-package-tools": "^1.16"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.43",
    "pestphp/pest": ">=2.35.1"
  },
  "autoload": {
    "psr-4": {
      "CheeasyTech\\Booking\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CheeasyTech\\Booking\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CheeasyTech\\Booking\\BookingServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/pest"
  }
}
